                <views:Cell Grid.Row="5" Grid.Column="2" DataContext="{Binding Cells[47]}"/>
                <views:Cell Grid.Row="5" Grid.Column="3" DataContext="{Binding Cells[48]}"/>
                <views:Cell Grid.Row="5" Grid.Column="4" DataContext="{Binding Cells[49]}"/>
